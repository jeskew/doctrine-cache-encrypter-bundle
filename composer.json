{
    "name": "jsq/doctrine-cache-encrypter-bundle",
    "description": "A bundle to manage encryption for your doctrine caches",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jonathan Eskew",
            "email": "jonathan@jeskew.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "jsq/doctrine-cache-encrypter": "~0.3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/framework-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jsq\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jsq\\Cache\\": "tests/"
        },
        "classmap": [
            "tests/fixtures"
        ]
    }
}
